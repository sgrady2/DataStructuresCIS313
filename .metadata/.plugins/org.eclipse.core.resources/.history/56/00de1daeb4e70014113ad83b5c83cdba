
public class Edge {
	Integer row;
	Integer col;
	Integer weight;
	public Edge(int in1, int in2){
		this.row = in1;
		this.col = in2;
		
	}
	//the row specfies where the edge came from
	//the col specfies where the edge is going directionally
	public void getPast(){
		System.out.println(row);
		}
	public void getNext(){
		System.out.println(col);
		}
}
